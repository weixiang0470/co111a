/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=sel,b=sel,out=Sbar);
    Nand(a=a,b=Sbar,out=y1);
    Nand(a=b,b=sel,out=y2);
    Nand(a=y1,b=y2,out=out);
}